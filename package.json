{
  "name": "react-user-control-manager",
  "version": "1.0.0",
  "description": "Generate Forms based on a JSON Spec",
  "main": "src/index.js",
  "contributors": [
    {
      "name": "Suresh Pillay",
      "email": "me@sureshpillay.com"
    },
    {
      "name": "Bhaskar Veligeti",
      "email": "bhaskarv20@gmail.com"
    }
  ],
  "scripts": {
    "webpack-watch": "webpack --mode development --watch",
    "start": "nodemon -e src/styles/ -x \"npm run sass\" --exec 'npm run webpack-watch'",
    "build": "npm run prebuild && npm run sass && webpack --mode production",
    "dev-server": "webpack-dev-server --hot --inline  --mode development",
    "build:tests": "rimraf test-dist/ && babel test/ --out-dir test-dist/",
    "test-mocha": "node node_modules/mocha/bin/mocha --compilers js:babel-core/register --reporter spec test-dist/*",
    "test-jest": "jest",
    "clean": "rimraf dist/ && rimraf test-dist/",
    "sass:dist-min": "node-sass  --output-style=compressed --importer=node_modules/node-sass-tilde-importer src/styles/styles.scss > dist/css/ucm.min.css",
    "sass:dist": "node-sass  --importer=node_modules/node-sass-tilde-importer src/styles/styles.scss > dist/css/ucm.css",
    "sass": "npm run sass:dist-min && npm run sass:dist",
    "prebuild": "mkdirp ./dist && mkdirp ./dist/css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BhaskarVeligeti/UserControlManagerUsage.git"
  },
  "keywords": [
    "React",
    "Forms",
    "Dynamic"
  ],
  "files": [
    "dist/**"
  ],
  "devDependencies": {
    "babel-cli": "6.16.0",
    "babel-core": "^6.18.0",
    "babel-eslint": "7.0.0",
    "babel-loader": "^7.0.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-inline-react-svg": "^0.5.2",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-plugin-transform-replace-object-assign": "^1.0.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.5.1",
    "babel-preset-react": "^6.24.1",
    "chai": "3.5.0",
    "eslint": "3.6.1",
    "eslint-config-airbnb": "12.0.0",
    "eslint-plugin-import": "1.16.0",
    "eslint-plugin-jsx-a11y": "2.2.2",
    "eslint-plugin-react": "6.3.0",
    "gulp": "3.9.1",
    "gulp-babel": "6.1.2",
    "gulp-eslint": "3.0.1",
    "gulp-mocha": "3.0.1",
    "gulp-rimraf": "0.2.0",
    "lodash.assign": "^4.2.0",
    "mkdirp": "^0.5.1",
    "mocha": "3.2.0",
    "node-sass": "^4.9.0",
    "node-sass-tilde-importer": "^1.0.2",
    "nodemon": "^1.17.4",
    "rimraf": "2.5.4",
    "run-sequence": "1.2.2",
    "webpack-cli": "^2.1.3"
  },
  "engine": "node >= 0.10.x",
  "author": "Suresh Pillay",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/spillay/FormManager/issues"
  },
  "homepage": "https://github.com/spillay/FormManager#readme",
  "dependencies": {
    "aphrodite": "^2.2.0",
    "css-loader": "^0.28.7",
    "file-loader": "^1.1.5",
