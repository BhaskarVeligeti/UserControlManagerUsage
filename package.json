    "jest": "^22.4.3",
    "json-loader": "^0.5.7",
    "prop-types": "^15.6.1",
    "raw-loader": "^0.5.1",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-helmet": "^5.2.0",
    "react-router-dom": "^4.2.2",
    "react-test-renderer": "^16.3.2",
    "react-with-direction": "^1.3.0",
    "react-with-styles": "^3.2.0",
    "react-with-styles-interface-aphrodite": "^5.0.0",
    "sass-loader": "^6.0.6",
